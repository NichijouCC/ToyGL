{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es6",
    "module": "es6",
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "dom"
    ],
    "strict": true,
    "strictNullChecks": false,
    "sourceMap": true,
    "declaration": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    // "declarationDir": "../exampesDist/types",
    "outDir": "../exampesDist",
    "typeRoots": [
      "../dist/types"
    ]
  }
}